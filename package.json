{
  "name": "maze-tv",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx build",
    "dep-graph": "nx dep-graph",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "lint:html": "yarn htmlhint --config .htmlhintrc --rulesdir ./tools/htmlhint-rules 'src/**/*.html'",
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "prettier": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "start": "nx serve",
    "test": "jest --verbose --coverage --config ./jest.config.ts",
    "update": "nx migrate latest"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.0.3",
    "@angular/cdk": "^14.0.3",
    "@angular/common": "14.0.3",
    "@angular/compiler": "14.0.3",
    "@angular/core": "14.0.3",
    "@angular/forms": "14.0.3",
    "@angular/material": "14.0.3",
    "@angular/platform-browser": "14.0.3",
    "@angular/platform-browser-dynamic": "14.0.3",
    "@angular/router": "14.0.3",
    "@nrwl/angular": "14.3.6",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.0.3",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "~14.0.0",
    "@angular/compiler-cli": "14.0.3",
    "@angular/language-service": "14.0.3",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-angular": "^17.0.0",
    "@ngneat/spectator": "^8.3.2",
    "@nrwl/cli": "14.3.6",
    "@nrwl/cypress": "14.3.6",
    "@nrwl/eslint-plugin-nx": "14.3.6",
    "@nrwl/jest": "14.3.6",
    "@nrwl/linter": "14.3.6",
    "@nrwl/nx-cloud": "14.1.2",
    "@nrwl/workspace": "14.3.6",
    "@types/jest": "27.4.1",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "5.24.0",
    "@typescript-eslint/parser": "5.24.0",
    "cypress": "^8.3.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "htmlhint": "^1.1.4",
    "husky": "^8.0.1",
    "jest": "27.5.1",
    "jest-preset-angular": "11.1.2",
    "lint-staged": "^13.0.2",
    "ng-packagr": "~14.0.0",
    "nx": "14.3.6",
    "postcss": "^8.4.5",
    "postcss-import": "~14.1.0",
    "postcss-preset-env": "~7.5.0",
    "postcss-url": "~10.1.3",
    "posthtml-attrs-sorter": "^1.1.0",
    "posthtml-cli": "^0.10.0",
    "prettier": "2.7.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.2.0",
    "svgo": "^2.8.0",
    "ts-jest": "27.1.4",
    "ts-node": "~10.8.0",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "*.{js,json,css,scss,less,md,ts}": [
      "prettier --write"
    ],
    "**/src/**/*.ts": [
      "eslint --fix"
    ],
    "**/src/**/*.scss": [
      "stylelint --cache --fix"
    ],
    "**/src/**/*.html": [
      "htmlhint --config .htmlhintrc --rulesdir ./tools/htmlhint-rules",
      "posthtml",
      "prettier --write"
    ],
    "**/src/**/*.svg": [
      "svgo"
    ]
  }
}
